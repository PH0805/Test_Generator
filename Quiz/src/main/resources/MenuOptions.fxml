<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="261.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MenuController">
   <children>
      <HBox alignment="CENTER" maxHeight="62.0" maxWidth="402.0" minHeight="62.0" minWidth="392.0" prefHeight="62.0" prefWidth="402.0" VBox.vgrow="ALWAYS">
         <children>
             <Button fx:id="StartQuizButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                     nodeOrientation="LEFT_TO_RIGHT" onAction="#openStartQuiz" prefHeight="50.0" prefWidth="120.0"
                     text="Rozpocznij test">
               <HBox.margin>
                  <Insets left="30.0" right="25.0" />
               </HBox.margin>
            </Button>
             <Button fx:id="TestGeneratorID" alignment="CENTER" mnemonicParsing="false" onAction="#openGenrator"
                     prefHeight="50.0" prefWidth="120.0" text="Test generator">
               <HBox.margin>
                  <Insets right="45.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" maxHeight="62.0" maxWidth="392.0" minHeight="62.0" minWidth="392.0" prefHeight="62.0" prefWidth="392.0">
         <children>
            <Button fx:id="OpenWynikiButton" alignment="CENTER" mnemonicParsing="false" onAction="#openWyniki" prefHeight="50.0" prefWidth="120.0" text="Wyniki">
               <HBox.margin>
                  <Insets left="55.0" right="25.0" />
               </HBox.margin>
            </Button>
             <Button fx:id="DrukujTestButton" alignment="CENTER" mnemonicParsing="false" onAction="#OpenPrintTest"
                     prefHeight="50.0" prefWidth="120.0" text="Drukuj test">
               <HBox.margin>
                  <Insets right="25.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" maxHeight="62.0" maxWidth="392.0" minHeight="62.0" minWidth="392.0" prefHeight="62.0" prefWidth="392.0">
         <children>
             <Button fx:id="QuitButton" alignment="CENTER" mnemonicParsing="false" onAction="#OpenWyjscie"
                     prefHeight="50.0" prefWidth="120.0" text="Zakończ">
               <HBox.margin>
                  <Insets left="55.0" right="25.0" />
               </HBox.margin></Button>
             <Button fx:id="PokazPytaniaButton" alignment="CENTER" mnemonicParsing="false" onAction="#openPokażPytania"
                     prefHeight="50.0" prefWidth="120.0" text="Edytor pytań">
               <HBox.margin>
                  <Insets right="25.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="15.0" />
   </padding>
</VBox>
